<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"

android:layout_width="match_parent"
android:layout_height="match_parent"
android:fitsSystemWindows="true"
tools:context="nu.paheco.patrik.mqttcodesend.MainActivity"
    >

<TextView
    android:layout_width="135dp"
    android:layout_height="wrap_content"
    android:text="Vardagsrum"
    android:id="@+id/text1"
    />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="1 On"
    android:id="@+id/btn1on"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/text1"
    />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="1 Off"
    android:id="@+id/btn1off"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/btn1on"
    />

<TextView
    android:layout_width="135dp"
    android:layout_height="wrap_content"
    android:text="Vardagsrum"
    android:id="@+id/text2"
    android:layout_below="@id/btn1on"
    />
<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="2 On"
    android:id="@+id/btn2on"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/text2"
    android:layout_below="@id/btn1on"
    />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="2 Off"
    android:id="@+id/btn2off"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/btn2on"
    android:layout_below="@id/btn1on"

    />

<TextView
    android:layout_width="135dp"
    android:layout_height="wrap_content"
    android:text="Vardagsrum"
    android:id="@+id/text3"
    android:layout_below="@id/btn2on"

    />
<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="3 On"
    android:id="@+id/btn3on"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/text3"
    android:layout_below="@id/btn2on"

    />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="3 Off"
    android:id="@+id/btn3off"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/btn3on"
    android:layout_below="@id/btn2on"

    />

<TextView
    android:layout_width="135dp"
    android:layout_height="wrap_content"
    android:text="Vardagsrum"
    android:id="@+id/text4"
    android:layout_below="@id/btn3on"

    />
<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="4 On"
    android:id="@+id/btn4on"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/text4"
    android:layout_below="@id/btn3on"

    />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="4 Off"
    android:id="@+id/btn4off"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/btn4on"
    android:layout_below="@id/btn3on"

    />
<TextView
    android:layout_width="135dp"
    android:layout_height="wrap_content"
    android:text="Vardagsrum"
    android:id="@+id/text5"
    android:layout_below="@id/btn4on"

    />
<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="5 On"
    android:id="@+id/btn5on"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/text5"
    android:layout_below="@id/btn4on"

    />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="5 Off"
    android:id="@+id/btn5off"
    android:enabled="true"
    android:onClick="btnClick"
    android:layout_toEndOf="@id/btn5on"
    android:layout_below="@id/btn4on"

    />
</RelativeLayout>
